{
  "manifest_version": 3,
  "name": "Extensao de Teste Minima",
  "version": "0.1",
  "description": "Extensao minima para teste",
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "scripting",
    "<all_urls>"
  ]
}